class Solution {
public:
    int climbStairs(int n) {
        vector<int> vec;
        for(int i=0;i<n;i++){
            if (i< 2){
                vec.push_back(1);
            }
            else
                vec.push_back(0);
        }
        
        for(int i=0;i<n;i++){
            if(i+2 < n){
                vec[i+2]+=vec[i];
            }
            if(i+1<n){
                vec[i+1]+=vec[i];
            }
        }
        return vec[n-1];
    }
};
